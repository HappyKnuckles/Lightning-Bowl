{
  "rewrites": [
    {
      "source": "/restapi/:path*",
      "destination": "https://www.bowwwl.com/restapi/:path*"
    }
  ],
  "headers": [
    {
      "source": "/restapi/:path*",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "no-store, no-cache, must-revalidate, proxy-revalidate"
        }
      ]
    }
  ]
}