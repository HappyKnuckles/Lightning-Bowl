{
  "rewrites": [
    {
      "source": "/restapi/:path*",
      "destination": "https://www.bowwwl.com/restapi/:path*"
    }
  ],
  "headers": [
    {
      "source": "/restapi/:path*",
      "headers": [
        {
          "key": "Host",
          "value": "www.bowwwl.com"
        },
        {
          "key": "Accept",
          "value": "application/json"
        },
        {
          "key": "Cache-Control",
          "value": "no-cache"
        }
      ]
    }
  ]
}
